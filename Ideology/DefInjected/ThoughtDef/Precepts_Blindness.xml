  <Blindness_Sublime_HalfBlind.stages.halfblind.label>półślepota</Blindness_Sublime_HalfBlind.stages.halfblind.label>
  <!-- EN: I am on the verge of true enlightment through blindness. -->
  <Blindness_Sublime_HalfBlind.stages.halfblind.description>Jestem u kresu prawdziwego oświecenia dzięki ślepocie.</Blindness_Sublime_HalfBlind.stages.halfblind.description>
  
  <!-- EN: half-blind -->
  <Blindness_Sublime_HalfBlind_Social.stages.halfblind.label>półślepota</Blindness_Sublime_HalfBlind_Social.stages.halfblind.label>
  
